{
  "name": "aether-beasts-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:updated": "ts-node prisma/seedWithConfig.ts updated",
    "db:seed:original": "ts-node prisma/seedWithConfig.ts original",
    "db:seed:new": "ts-node prisma/seedWithConfig.ts new",
    "db:seed:backup": "ts-node prisma/seedWithConfig.ts backup",
    "db:seed:with-new": "ts-node prisma/seedWithConfig.ts withNewCards",
    "db:reset": "prisma migrate reset --force",
    "db:view": "ts-node prisma/viewDatabase.ts",
    "db:compare": "ts-node prisma/compareSeedConfigs.ts"
  },
  "keywords": [
    "card-game",
    "websocket",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "description": "Backend for Aether Beasts card game",
  "dependencies": {
    "@getbrevo/brevo": "^2.5.0",
    "@prisma/client": "^6.12.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "prisma": "^6.12.0",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.16.4",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0"
  },
  "prisma": {
